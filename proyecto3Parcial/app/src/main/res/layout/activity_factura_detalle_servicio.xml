<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FacturaDetalleServicioActivity">

    <Spinner
        android:id="@+id/spCodigoFacturaDetalleServicio"
        android:layout_width="0dp"
        android:layout_height="51dp"
        android:layout_marginTop="165dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txvCodigoServicioDetalle"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/txvCodigoServicioDetalle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="71dp"
        android:text="Codigo Servicio"
        app:layout_constraintBottom_toBottomOf="@+id/spCodigoFacturaDetalleServicio"
        app:layout_constraintEnd_toStartOf="@+id/spCodigoFacturaDetalleServicio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spCodigoFacturaDetalleServicio" />

    <Button
        android:id="@+id/btnVerServicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="83dp"
        android:text="Ver Servicio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txvCodigoServicioDetalle" />
</androidx.constraintlayout.widget.ConstraintLayout>